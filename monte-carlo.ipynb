    "# function that always gives the  correct data set\n",
    "def rejection_sampling_data(N):\n",
    "    x, y = create_data(N)\n",
    "    for k in range(N-1):\n",
    "        while y[k]==y[k+1]:\n",
    "            a, b = create_data(1)\n",
    "            x[k+1] = a\n",
    "            y[k+1] = b\n",
    "    return x, y\n",
